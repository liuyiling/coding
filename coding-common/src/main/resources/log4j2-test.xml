<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern= "%d{yyyy-MM-dd HH:mm:ss} [%p] %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root />
        <Logger name="fire" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="error" level="error" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="warn" level="warn" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="info" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="api" level="trace" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="profile" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="post" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="monitor_msg" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
         <Logger name="monitor_stat" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="fail_msg" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="jedis" level="error" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="bigdata" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="degrade_msg" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="replay" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="order" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="delete" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="giftfake" level="info" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="grpcMsg" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="coding-redis" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>